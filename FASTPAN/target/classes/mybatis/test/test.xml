<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >				
<mapper namespace="com.fastpan.dao.mapper.TestMapper">
  
  
  <select id="queryob" resultType="com.fastpan.dao.pojo.Test">
  	select * from test
  
  </select>
  
  
  
    <insert id="insertTest"  parameterType="com.fastpan.dao.pojo.Test">
  	insert into test(id,name)
  	
  	values(#{id},#{name})
  
  </insert>
  

  <select id="selectAll"  resultType="com.fastpan.dao.pojo.Test"  >
  	select * from test
  
  </select>
  
</mapper>